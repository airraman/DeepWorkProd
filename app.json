{
  "expo": {
    "name": "DeepWork.io",
    "slug": "DeepWorkApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2563EB",
          "sounds": []
        }
      ],
      "./ios-background-tasks-plugin.js",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1", 
            "newArchEnabled": false
          }
        }
      ]
    ],
    "assets": [
      "./assets/alarm.mp3",
      "./assets/lofi.mp3", 
      "./assets/whitenoise.mp3"
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.airraman.deepwork",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing",
          "audio"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.expo.tasks.BACKGROUND_TIMER_TASK"
        ],
        "NSUserNotificationsUsageDescription": "This app uses notifications to remind you when your deep work sessions are complete and to track session progress.",
        "NSUserTrackingUsageDescription": "This app does not track users.",
        "CFBundleDisplayName": "DeepWork.io",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.airraman.deepwork",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6154a390-07d0-416c-955b-63179fba2bc8"
      }
    },
    "owner": "airraman",
    "runtimeVersion": "1.0.1",
    "updates": {
      "url": "https://u.expo.dev/6154a390-07d0-416c-955b-63179fba2bc8"
    }
  }
}